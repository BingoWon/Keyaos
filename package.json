{
	"name": "keyaos",
	"version": "0.1.0",
	"private": true,
	"license": "BUSL-1.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build && node scripts/build-search-index.mjs",
		"deploy": "wrangler d1 migrations apply keyaos-db --remote && wrangler deploy",
		"test": "vitest run",
		"test:api": "node --env-file=.env.local --experimental-strip-types --no-warnings --test tests/e2e/**/*.test.ts",
		"lint": "biome check --write .",
		"format": "biome format --write .",
		"db:setup:local": "wrangler d1 execute keyaos-db --local --file=./migrations/0001_initial.sql",
		"db:setup:remote": "wrangler d1 execute keyaos-db --remote --file=./migrations/0001_initial.sql"
	},
	"dependencies": {
		"@clerk/clerk-react": "^5.61.1",
		"@clerk/localizations": "^3.37.0",
		"@clerk/themes": "^2.4.55",
		"@headlessui/react": "^2.2.9",
		"@heroicons/react": "^2.2.0",
		"@hono/clerk-auth": "^3.1.0",
		"@iconify/react": "^6.0.2",
		"@mdx-js/rollup": "^3.1.1",
		"crisp-sdk-web": "^1.0.27",
		"hono": "^4.12.0",
		"i18next": "^25.8.11",
		"lightweight-charts": "^5.1.0",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-hot-toast": "^2.6.0",
		"react-i18next": "^16.5.4",
		"react-router-dom": "^7.13.0",
		"remark-gfm": "^4.0.1",
		"zod": "^4.3.6",
		"zustand": "^5.0.11"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "^0.78.0",
		"@biomejs/biome": "^2.4.3",
		"@cloudflare/vite-plugin": "^1.25.2",
		"@cloudflare/workers-types": "^4.20260219.0",
		"@tailwindcss/vite": "4",
		"@types/mdx": "^2.0.13",
		"@types/node": "^25.3.0",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.4",
		"openai": "^6.24.0",
		"pagefind": "^1.4.0",
		"tailwindcss": "4",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"wrangler": "^4.67.0"
	}
}
